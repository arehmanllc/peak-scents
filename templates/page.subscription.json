          "settings": {
            "icon_name": "icon-star-rating",
            "icon_color": "#aaaaaa",
            "title": "Family Owned Business",
            "text": "<p>Because Peak Scents is a small, family-owned business with commitment to quality, our care for customer satisfaction, education, and support falls under that commitment to quality as well!</p>",
            "link_text": "",
            "link_url": ""
          }
        },
