          "settings": {
            "icon_name": "icon-star-rating",
            "icon_color": "#aaaaaa",
            "title": "Great Communication",
            "text": "<p>We pride ourselves on quick and educational communication. You can talk one-on-one to one of our knowledgeable and passionate team members with any questions you may have anytime, just call our toll free number- 800-630-9499.</p>",
            "link_text": "",
            "link_url": ""
